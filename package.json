{
    "name": "project-glitter",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@assemblyscript/loader": "^0.19.2",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "@types/jest": "^26.0.23",
        "@types/node": "^12.20.15",
        "@types/react": "^17.0.11",
        "@types/react-dom": "^17.0.7",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "4.0.3",
        "typescript": "^4.3.2",
        "web-vitals": "^1.1.2"
    },
    "devDependencies": {
